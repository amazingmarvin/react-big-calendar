{
  "name": "react-big-calendar",
  "version": "0.19.2",
  "description": "Calendar! with events",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "intljusticemission/react-big-calendar",
  "license": "MIT",
  "main": "lib/index.js",
  "style": "lib/css/react-big-calendar.css",
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "calendar",
    "events",
    "full calendar"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "clean:examples": "rimraf examples/static",
    "l": "lessc --autoprefix=\"ie >= 10, safari >= 8, last 2 versions\" ",
    "less": "npm run l src/less/styles.less ./lib/css/react-big-calendar.css && npm run less-dnd",
    "less-dnd": "npm run l src/addons/dragAndDrop/styles.less ./lib/addons/dragAndDrop/styles.css",
    "assets": "cpy src/less/* lib/less && npm run assets-addons",
    "assets-addons": "cpy addons/**/*.less ../lib/ --cwd=src --parents",
    "build": "npm run clean && babel src --out-dir lib && npm run assets && npm run less",
    "build:marvin": "npm run clean && babel src --out-dir lib && npm run assets && npm run less && cd ../Marvin && yarn install",
    "build:examples": "npm run clean:examples && webpack --config examples/webpack.config.js",
    "examples": "npm run clean:examples && webpack-dev-server --inline --hot --config examples/webpack.config.js",
    "lint": "eslint src test",
    "test": "npm run lint && jest",
    "tdd": "jest --watch",
    "prettier": "prettier '**/*js' !examples/bundle.js '!lib/**' --write ",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": "eslint",
    "test/**/*.js": "eslint",
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^15.3.0 || ^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.23.4",
    "@babel/core": "7.23.6",
    "@babel/eslint-parser": "7.23.3",
    "@typescript-eslint/eslint-plugin": "6.16.0",
    "babel-plugin-object-assign": "1.2.1",
    "babel-preset-jason": "6.3.0",
    "bootstrap": "5.3.2",
    "component-metadata-loader": "4.0.0",
    "cpy": "11.0.0",
    "cpy-cli": "5.0.0",
    "eslint": "8.56.0",
    "eslint-config-jason": "8.2.2",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-webpack": "0.13.8",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-react": "7.33.2",
    "font-awesome": "4.7.0",
    "globalize": "1.7.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "less": "4.2.0",
    "less-loader": "11.1.3",
    "less-plugin-autoprefix": "2.0.0",
    "lint-staged": "15.2.0",
    "markdown-jsx-loader": "3.0.2",
    "marked": "11.1.0",
    "moment": "2.29.4",
    "mt-changelog": "0.6.2",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-bootstrap": "2.9.1",
    "react-docgen": "7.0.1",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "classnames": "2.3.2",
    "date-arithmetic": "4.1.0",
    "dom-helpers": "5.2.1",
    "invariant": "2.2.4",
    "lodash": "4.17.21",
    "memoize-one": "6.0.0",
    "prop-types": "15.8.1",
    "react-overlays": "5.2.1",
    "react-prop-types": "0.4.0",
    "react-tackle-box": "2.1.0",
    "uncontrollable": "7.2.1",
    "warning": "4.0.3"
  },
  "resolutions": {
    "strip-ansi": "6.0.1"
  }
}
